source ~/.config/nvim/init.vim
source ~/.config/nvim/plugin/settings.vim


set NERDTree
set surround
set commentary
set ReplaceWithRegister
set argtextobj
set highlightedyank

nnoremap <leader>fb :action Switcher<CR>
nnoremap <leader>fr :action RecentFiles<CR>

nnoremap <leader>rr :action RenameElement<CR>
nnoremap <leader>rei :action ExtractInterface<CR>
nnoremap <leader>rev :action IntroduceVariable<CR>
nnoremap <leader>rem :action ExtractMethod<CR>

nnoremap <leader>fu :action FindUsages<CR>
nnoremap <leader>su :action ShowUsages<CR>
